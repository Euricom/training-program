{
  "name": "js-react-webpack",
  "version": "1.0.0",
  "description": "React WebPack Template",
  "scripts": {
    "serve": "webpack-dev-server --open --inline --hot",
    "build": "webpack",
    "build:prod": "webpack -p",
    "lint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,json,css, scss, html}'",
    "test:ci": "jest",
    "test": "jest --watch",
    "analyse": "webpack-bundle-analyzer bundle/stats.json"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "intl": "^1.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@types/react": "^16.7.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-airbnb": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "prettier": "^1.15.3",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "verbose": false,
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest.setup.js",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
