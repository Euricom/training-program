{
  "extends": ["airbnb", "prettier"],
  "globals": {},
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-console": [0, ""],
    "array-callback-return": "off",
    "consistent-return": "off"
  }
}
